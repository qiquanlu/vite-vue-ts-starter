{
  "name": "vite-ts",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "view:eslint": "eslint-flat-config-viewer",
    "up": "taze major -I",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@iconify-json/carbon": "^1.1.27",
    "@iconify-json/mdi": "^1.1.63",
    "@types/node": "^20.10.5",
    "@unocss/preset-mini": "^0.58.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "eslint": "^8.56.0",
    "lint-staged": "^15.2.0",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.0",
    "typescript": "*",
    "unocss": "^0.58.0",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
